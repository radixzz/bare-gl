ex.\n    this.enabledCapabilities = {};\n  }\n\n  setCapability(cap, enabled) {\n    const { enabledCapabilities, gl } = this;\n    if (enabledCapabilities[cap] !== enabled) {\n      enabledCapabilities[cap] = enabled;\n      if (enabled) {\n        gl.enable(cap);\n      } else {\n        gl.disable(cap);\n      }\n    }\n  }\n\n  setBlend(enabled) {\n    this.setCapability(GL.BLEND, enabled);\n  }\n\n  setBlendFunc(blendFunc) {\n    const { gl } = this;\n    if (this.blendFunc !== blendFunc) {\n      this.blendFunc = blendFunc;\n      gl.blendFuncSeparate.apply(this, BLEND_PRESETS[blendFunc]);\n    }\n  }\n\n  setBlendEquation(blendEquation) {\n    const { gl } = this;\n    if (this.blendEquation !== blendEquation) {\n      this.blendEquation = blendEquation;\n      gl.blendEquationSeparate.apply(this, blendEquation);\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"5310ea764d0bb17e12dae02b288e74b6","cacheData":{"env":{}}}